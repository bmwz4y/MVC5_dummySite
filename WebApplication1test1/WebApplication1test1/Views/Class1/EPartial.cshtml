@model IEnumerable<WebApplication1test1.Models.Class1>

@{
    //to view using the custom layout for lesson 59
    Layout = "~/Views/Shared/_Layout_CUSTOM.cshtml";
}

<br />
<table>
    <tr>
        <td>
            @*lesson 52*@
            @foreach (var item in Model)
            {
                @Html.Partial("_Epartial", item)
            }
        </td>

        <td style="width: 10px" />

        <td>
            @*lesson 53 to differentiate between renderpartial and partial (informational)*@
            @foreach (var item in Model)
            {
                {
                    Html.RenderPartial("_Epartial", item);
                }
            }
        </td>
    </tr>
</table>

@*copyright notice using last modified date for each view on it's own*@
@section copyWrite
    {
    @File.GetLastWriteTime(Server.MapPath(VirtualPath)).ToLongDateString()
}